@model EasyHarmonica.WEB.Models.StartViewModel

<table>
@foreach (var lesson in Model.Lessons)
{
    <tr>
        <td>
            @Html.ActionLink(lesson.Name, "GetLesson", "Lesson", new { name = lesson.Name }, null)
        </td>
        <td>
            @Html.DisplayFor(item=>lesson.Info)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditLesson", "Lesson", new{name = lesson.Name}, null)
        </td>
    </tr>
}
</table>
